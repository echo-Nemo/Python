ls=[1,2,3]
def Text():
    for i in "PYTHON":
        ls.append(i)#ls是全局变量
    return ls
print(Text())
def Test():
    ls=[21,23]
    for i in range(1,4):
        ls.append(i)#ls是局部变量
    return ls
print(Test())    

s=1
def N(m):
    global s
    for i in range(1,m+1):
        s*=i#全局变量s
    return s
print(N(4))
print(s)#s是全局变量
        
    
